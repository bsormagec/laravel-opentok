{
    "name": "sormagec/opentok-laravel",
    "description": "Laravel Wrapper for the OpenTok PHP SDK",
    "keywords": ["opentok", "streaming", "video streaming", "laravel"],
    "homepage": "https://github.com/bsormagec/opentok-laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Burak SormageÃ§",
            "email": "burak@sormagec.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x",
        "opentok/opentok": "4.*"
    },
    "autoload": {
